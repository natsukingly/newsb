
.sub_function_wrapper.search
	.sub_function_main_section
		.sub_function_nav
			%h2
				RANKING
			
			.nav_options	
				=link_to "WEEKLY", ranking_weekly_user_path 
				=link_to "ALL-TIME", ranking_user_path, class: "active"

		#main_contents.user_lists
			= render partial: "users/user_list", collection: @users, as: :user
	
	.sub_function_sidebar
		%h2
			NEW USERS
		.new_users_section
			-unless @new_users.empty?
				= render partial: "users/card", collection: @new_users, as: :user
