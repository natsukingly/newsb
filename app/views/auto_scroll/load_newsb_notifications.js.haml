$('.scroll_btn.newsb_notifications').before("#{ escape_javascript(render partial: 'admins/newsb_notification', collection: @loaded_newsb_notifications.includes(:target_user), as: :notification)}");
$('.scroll_btn.newsb_notifications').html("#{t('scroll.load')}");
$('.scroll_btn.newsb_notifications').removeClass('loading');
-if @loaded_newsb_notifications.empty? || @loaded_newsb_notifications.count < 30
    $('.scroll_btn.newsb_notifications').html("#{t('scroll.no_more')}");
    $('.scroll_btn.newsb_notifications').addClass('stop_loading');
    
