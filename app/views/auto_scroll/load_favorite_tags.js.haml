$('.scroll_btn.tags').before('#{ escape_javascript(render @loaded_tags)}');
$('.scroll_btn.tags').html("#{t('scroll.load')}");
$('.scroll_btn.tags').removeClass('loading');
-if @loaded_tags.empty? || @loaded_tags.count < 30
    $('.scroll_btn.tags').html("#{t('scroll.no_more')}");
    $('.scroll_btn.tags').addClass('stop_loading');
    
loading = false;
