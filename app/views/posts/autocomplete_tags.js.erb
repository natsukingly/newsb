<% if @autocomplete_tag_lists.empty? %>
    $('.autocomplete_tags_block').html(" ");
<% else %>
    $('[data-form="<%= @form %>"]').find('.autocomplete_tags_block').html('<%= escape_javascript(render("posts/autocomplete_tag_lists"))%>');
    $('.suggested_tags').find('li').eq(0).addClass("selected");
<% end %>

