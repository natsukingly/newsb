%h1 Listing users

%table
  %thead
    %tr
      %th Name
      %th Image
      %th Provider
      %th Uid
      %th
      %th
      %th

  %tbody
    - @users.each do |user|
      %tr
        %td= user.name
        %td= user.image
        %td= user.provider
        %td= user.uid
        %td= link_to 'Show', user
        %td= link_to 'Edit', edit_user_path(user)
        %td= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' }


